{
    "name": "uvdesk/composer-plugin",
    "type": "composer-plugin",
    "description": "Generic purpose composer plugin to auto-configure your packages",
    "license": "MIT",
    "authors": [
        {
            "name": "Akshay Kumar",
            "email": "akshay.kumar758@webkul.com"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "composer-plugin-api": "^1.0 || ^2.0"
    },
    "require-dev": {
        "composer/composer": "1.0.* || 2.0.*"
    },
    "autoload": {
        "psr-4": {
            "Webkul\\UVDesk\\PackageManager\\": "lib/"
        }
    },
    "extra": {
        "class": "Webkul\\UVDesk\\PackageManager\\Manager"
    },
    "minimum-stability": "stable"
}
